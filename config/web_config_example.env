# Example environment configuration for web deployment
# Copy this to .env and adjust paths as needed

# Base directory paths (relative to where the app runs)
APP_BASE_DIR=/app
CONFIG_DIR=/app/config
DATA_DIR=/app/data
LOGS_DIR=/app/logs

# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# External directories (optional)
SUBMISSION_DIR=/path/to/external/submissions

# LangChain configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_PROJECT=OfferNegotiationAgent 